
struct S
    var i: float32 = 7.
end

struct T
    var x: float64 = 42.0
    var s: S
end

activity B(i: float64)() returns bool
    await true
    return i < 7.654
end

activity A()(x: float64)
    x = 1.23
    await true
    var y: bool
    run y = B(x)() // output inarg simple direct cur
    await true
end

@[EntryPoint]
activity entry()
    var v: float64
    run A()(v)
end

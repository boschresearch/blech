struct Foo
    var b: bool
    var a: int32
    var r: float32
end

const aa: [7]int32 = {[2]=5, 8, 9, 8}

const ff: Foo = {b = true, a = 7, r = -4.3}

function f(x: [7]int32)() returns int32
    let loc = aa
    if loc[0] == 0 then return loc[3] * x[3]
    else return x[4] end
end

function g()() returns int32
    var t = f({[3]=75, 78, 79, 778})
    return t
end

@[EntryPoint]
activity entry()()
    await true
    var res = g()
end

struct S
    var i: float64 = 7.
end

struct T
    var x: float64 = 42.0
    var s: S
end

activity B(b: float64)() returns bool
    await true
    return b > 0.0
end

activity A(x: S)()
    var r: bool
    run r =  B(x.i) // input inarg simple fieldaccess cur
    await true
end

@[EntryPoint]
activity entry()
    var v: S
    run A(v)()
end

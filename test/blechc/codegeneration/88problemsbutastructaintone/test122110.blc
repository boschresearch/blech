
struct S
    var i: float32 = 7.f
end

struct T
    var x: float64 = 42.0
    var s: S
end

activity B()(o: S)
    o.i = 7.654
    await true
end

activity A()(x: T)
    await true
    run B()(x.s) // output outarg struct fieldaccess cur
    await true
end

@[EntryPoint]
activity entry()
    var v: T
    run A()(v)
end

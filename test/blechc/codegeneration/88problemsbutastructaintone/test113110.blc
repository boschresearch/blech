
struct S
    var i: float64 = 7.
end

struct T
    var x: float64 = 42.0
    var s: S
end

activity B(b: S)() returns bool
    await true
    return b.i > 0.0
end

activity A(x: T)()
    var r: bool
    run r = B(x.s) // input inarg struct fieldaccess cur
    await true
end

@[EntryPoint]
activity entry()
    var v: T
    run A(v)()
end
